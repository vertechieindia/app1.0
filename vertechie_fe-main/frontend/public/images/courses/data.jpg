<svg width="800" height="450" viewBox="0 0 800 450" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#0F2027" />
      <stop offset="50%" stop-color="#203A43" />
      <stop offset="100%" stop-color="#2C5364" />
    </linearGradient>
    <linearGradient id="glowGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#48B1BF" />
      <stop offset="100%" stop-color="#06BEB6" />
    </linearGradient>
    <filter id="glow" x="-20%" y="-20%" width="140%" height="140%">
      <feGaussianBlur stdDeviation="5" result="blur" />
      <feComposite in="SourceGraphic" in2="blur" operator="over" />
    </filter>
    <pattern id="grid" width="40" height="40" patternUnits="userSpaceOnUse">
      <path d="M 40 0 L 0 0 0 40" fill="none" stroke="#4C566A" stroke-width="1" />
    </pattern>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="450" fill="url(#bgGradient)" />
  <rect width="800" height="450" fill="url(#grid)" opacity="0.2" />
  
  <!-- Central Database -->
  <g transform="translate(400, 225)" filter="url(#glow)">
    <!-- Database cylinder -->
    <g>
      <ellipse cx="0" cy="-40" rx="70" ry="25" fill="#2E3440" />
      <rect x="-70" y="-40" width="140" height="80" fill="#2E3440" />
      <ellipse cx="0" cy="40" rx="70" ry="25" fill="#2E3440" />
      
      <!-- Inner cylinder -->
      <ellipse cx="0" cy="-40" rx="60" ry="20" fill="#3B4252" />
      <rect x="-60" y="-40" width="120" height="80" fill="#3B4252" />
      <ellipse cx="0" cy="40" rx="60" ry="20" fill="#3B4252" />
      
      <!-- Data segments -->
      <ellipse cx="0" cy="-40" rx="50" ry="15" fill="#434C5E" />
      <path d="M-50,-40 L-50,40 A50,15 0 0,0 50,40 L50,-40 A50,15 0 0,1 -50,-40 Z" fill="#434C5E" />
      
      <!-- Database lines -->
      <ellipse cx="0" cy="-20" rx="50" ry="15" fill="none" stroke="#81A1C1" stroke-width="1" opacity="0.5" />
      <ellipse cx="0" cy="0" rx="50" ry="15" fill="none" stroke="#81A1C1" stroke-width="1" opacity="0.5" />
      <ellipse cx="0" cy="20" rx="50" ry="15" fill="none" stroke="#81A1C1" stroke-width="1" opacity="0.5" />
    </g>
  </g>
  
  <!-- Data flow lines -->
  <g stroke="url(#glowGradient)" stroke-width="3" fill="none" opacity="0.8">
    <!-- Input to database -->
    <path d="M150,150 C250,100 300,150 400,185" stroke-dasharray="5,5" />
    <path d="M150,225 C250,225 300,225 330,225" />
    <path d="M150,300 C250,350 300,300 400,265" stroke-dasharray="5,5" />
    
    <!-- Database to output -->
    <path d="M470,185 C550,150 600,150 650,150" />
    <path d="M470,225 C550,225 600,225 650,225" stroke-dasharray="5,5" />
    <path d="M470,265 C550,300 600,300 650,300" />
  </g>
  
  <!-- Input sources -->
  <g transform="translate(150, 150)">
    <rect x="-50" y="-30" width="100" height="60" rx="5" ry="5" fill="#D8DEE9" filter="url(#glow)" />
    <text x="0" y="5" font-family="monospace" font-weight="bold" font-size="14" fill="#2E3440" text-anchor="middle">CSV FILES</text>
    <g transform="translate(-30, -10)">
      <rect x="-10" y="-5" width="20" height="10" fill="#5E81AC" />
      <rect x="-10" y="10" width="20" height="10" fill="#5E81AC" />
    </g>
  </g>
  
  <g transform="translate(150, 225)">
    <rect x="-50" y="-30" width="100" height="60" rx="5" ry="5" fill="#D8DEE9" filter="url(#glow)" />
    <text x="0" y="5" font-family="monospace" font-weight="bold" font-size="14" fill="#2E3440" text-anchor="middle">API</text>
    <g transform="translate(-30, -10)">
      <rect x="-10" y="0" width="20" height="2" fill="#5E81AC" />
      <rect x="-10" y="5" width="15" height="2" fill="#5E81AC" />
      <rect x="-10" y="10" width="20" height="2" fill="#5E81AC" />
    </g>
  </g>
  
  <g transform="translate(150, 300)">
    <rect x="-50" y="-30" width="100" height="60" rx="5" ry="5" fill="#D8DEE9" filter="url(#glow)" />
    <text x="0" y="5" font-family="monospace" font-weight="bold" font-size="14" fill="#2E3440" text-anchor="middle">STREAMS</text>
    <g transform="translate(-30, -10)">
      <path d="M-15,-5 L15,-5 M-10,0 L10,0 M-15,5 L15,5 M-10,10 L10,10" stroke="#5E81AC" stroke-width="2" />
    </g>
  </g>
  
  <!-- Output analytics -->
  <g transform="translate(650, 150)">
    <rect x="-50" y="-30" width="100" height="60" rx="5" ry="5" fill="#D8DEE9" filter="url(#glow)" />
    <text x="0" y="5" font-family="monospace" font-weight="bold" font-size="14" fill="#2E3440" text-anchor="middle">DASHBOARD</text>
    <g transform="translate(-30, -10)">
      <rect x="-10" y="-5" width="20" height="2" fill="#5E81AC" />
      <rect x="-10" y="0" width="15" height="10" fill="#5E81AC" />
      <rect x="-10" y="15" width="20" height="2" fill="#5E81AC" />
    </g>
  </g>
  
  <g transform="translate(650, 225)">
    <rect x="-50" y="-30" width="100" height="60" rx="5" ry="5" fill="#D8DEE9" filter="url(#glow)" />
    <text x="0" y="5" font-family="monospace" font-weight="bold" font-size="14" fill="#2E3440" text-anchor="middle">ML MODEL</text>
    <g transform="translate(-30, -10)">
      <circle cx="0" cy="0" r="10" fill="none" stroke="#5E81AC" stroke-width="2" />
      <circle cx="0" cy="0" r="5" fill="#5E81AC" />
    </g>
  </g>
  
  <g transform="translate(650, 300)">
    <rect x="-50" y="-30" width="100" height="60" rx="5" ry="5" fill="#D8DEE9" filter="url(#glow)" />
    <text x="0" y="5" font-family="monospace" font-weight="bold" font-size="14" fill="#2E3440" text-anchor="middle">REPORTS</text>
    <g transform="translate(-30, -10)">
      <rect x="-10" y="-5" width="20" height="15" fill="#5E81AC" />
      <rect x="-7" y="-2" width="14" height="1" fill="#D8DEE9" />
      <rect x="-7" y="1" width="14" height="1" fill="#D8DEE9" />
      <rect x="-7" y="4" width="14" height="1" fill="#D8DEE9" />
      <rect x="-7" y="7" width="10" height="1" fill="#D8DEE9" />
    </g>
  </g>
  
  <!-- Data nodes -->
  <g fill="url(#glowGradient)" opacity="0.9">
    <circle cx="250" cy="150" r="5">
      <animate attributeName="opacity" values="1;0.3;1" dur="3s" repeatCount="indefinite" />
    </circle>
    <circle cx="350" cy="185" r="5">
      <animate attributeName="opacity" values="0.3;1;0.3" dur="3s" repeatCount="indefinite" />
    </circle>
    <circle cx="250" cy="225" r="5">
      <animate attributeName="opacity" values="1;0.3;1" dur="4s" repeatCount="indefinite" />
    </circle>
    <circle cx="350" cy="225" r="5">
      <animate attributeName="opacity" values="0.3;1;0.3" dur="4s" repeatCount="indefinite" />
    </circle>
    <circle cx="250" cy="300" r="5">
      <animate attributeName="opacity" values="1;0.3;1" dur="3.5s" repeatCount="indefinite" />
    </circle>
    <circle cx="350" cy="265" r="5">
      <animate attributeName="opacity" values="0.3;1;0.3" dur="3.5s" repeatCount="indefinite" />
    </circle>
    
    <circle cx="550" cy="150" r="5">
      <animate attributeName="opacity" values="1;0.3;1" dur="3s" repeatCount="indefinite" />
    </circle>
    <circle cx="550" cy="225" r="5">
      <animate attributeName="opacity" values="0.3;1;0.3" dur="4s" repeatCount="indefinite" />
    </circle>
    <circle cx="550" cy="300" r="5">
      <animate attributeName="opacity" values="1;0.3;1" dur="3.5s" repeatCount="indefinite" />
    </circle>
  </g>
  
  <!-- Course title -->
  <g filter="url(#glow)">
    <rect x="200" y="380" width="400" height="50" rx="5" ry="5" fill="#2E3440" opacity="0.9" />
    <text x="400" y="415" font-family="Arial, sans-serif" font-weight="bold" font-size="28" fill="#ECEFF4" text-anchor="middle">DATA ENGINEERING</text>
  </g>
  
  <!-- VerTechie brand -->
  <g transform="translate(60, 40)">
    <rect x="-40" y="-20" width="120" height="40" rx="5" ry="5" fill="#3B4252" opacity="0.8" />
    <text x="20" y="5" font-family="Arial, sans-serif" font-weight="bold" font-size="16" fill="#ECEFF4" text-anchor="middle">VerTechie</text>
  </g>
  
  <!-- Pipeline Labels -->
  <g font-family="Arial, sans-serif" font-size="12" fill="#ECEFF4" opacity="0.7">
    <text x="250" y="100" text-anchor="middle">DATA SOURCES</text>
    <text x="400" y="100" text-anchor="middle">PROCESSING</text>
    <text x="550" y="100" text-anchor="middle">ANALYTICS</text>
  </g>
</svg> 