<svg width="800" height="450" viewBox="0 0 800 450" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#1E3C72" />
      <stop offset="100%" stop-color="#2A5298" />
    </linearGradient>
    <linearGradient id="cloudGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#FFFFFF" />
      <stop offset="100%" stop-color="#E6E9F0" />
    </linearGradient>
    <linearGradient id="serverGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#434C5E" />
      <stop offset="100%" stop-color="#2E3440" />
    </linearGradient>
    <linearGradient id="dataGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#88BFFF" />
      <stop offset="100%" stop-color="#5E81AC" />
    </linearGradient>
    <filter id="glow" x="-20%" y="-20%" width="140%" height="140%">
      <feGaussianBlur stdDeviation="5" result="blur" />
      <feComposite in="SourceGraphic" in2="blur" operator="over" />
    </filter>
    <pattern id="grid" width="40" height="40" patternUnits="userSpaceOnUse">
      <path d="M 40 0 L 0 0 0 40" fill="none" stroke="#2C4790" stroke-width="1" />
    </pattern>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="450" fill="url(#bgGradient)" />
  <rect width="800" height="450" fill="url(#grid)" opacity="0.3" />
  
  <!-- Cloud pattern left side -->
  <g opacity="0.1" fill="#FFFFFF">
    <path d="M120,100 Q140,70 170,80 Q190,60 220,75 Q250,60 260,90 Q290,85 295,110 Q270,130 230,125 Q215,150 185,140 Q165,160 140,140 Q110,145 105,125 Q115,105 120,100" />
    <path d="M80,200 Q100,170 130,180 Q150,160 180,175 Q210,160 220,190 Q250,185 255,210 Q230,230 190,225 Q175,250 145,240 Q125,260 100,240 Q70,245 65,225 Q75,205 80,200" />
    <path d="M150,300 Q170,270 200,280 Q220,260 250,275 Q280,260 290,290 Q320,285 325,310 Q300,330 260,325 Q245,350 215,340 Q195,360 170,340 Q140,345 135,325 Q145,305 150,300" />
  </g>
  
  <!-- Cloud pattern right side -->
  <g opacity="0.1" fill="#FFFFFF">
    <path d="M520,100 Q540,70 570,80 Q590,60 620,75 Q650,60 660,90 Q690,85 695,110 Q670,130 630,125 Q615,150 585,140 Q565,160 540,140 Q510,145 505,125 Q515,105 520,100" />
    <path d="M580,200 Q600,170 630,180 Q650,160 680,175 Q710,160 720,190 Q750,185 755,210 Q730,230 690,225 Q675,250 645,240 Q625,260 600,240 Q570,245 565,225 Q575,205 580,200" />
    <path d="M550,300 Q570,270 600,280 Q620,260 650,275 Q680,260 690,290 Q720,285 725,310 Q700,330 660,325 Q645,350 615,340 Q595,360 570,340 Q540,345 535,325 Q545,305 550,300" />
  </g>
  
  <!-- On-premises infrastructure (left side) -->
  <g transform="translate(200, 225)">
    <!-- Server rack -->
    <rect x="-60" y="-70" width="120" height="140" rx="5" ry="5" fill="url(#serverGradient)" filter="url(#glow)" />
    
    <!-- Rack units -->
    <rect x="-55" y="-65" width="110" height="20" rx="2" ry="2" fill="#4C566A" />
    <rect x="-55" y="-40" width="110" height="20" rx="2" ry="2" fill="#4C566A" />
    <rect x="-55" y="-15" width="110" height="20" rx="2" ry="2" fill="#4C566A" />
    <rect x="-55" y="10" width="110" height="20" rx="2" ry="2" fill="#4C566A" />
    <rect x="-55" y="35" width="110" height="20" rx="2" ry="2" fill="#4C566A" />
    
    <!-- Server details -->
    <rect x="-45" y="-60" width="5" height="10" rx="1" ry="1" fill="#A3BE8C" />
    <rect x="-45" y="-35" width="5" height="10" rx="1" ry="1" fill="#EBCB8B" />
    <rect x="-45" y="-10" width="5" height="10" rx="1" ry="1" fill="#A3BE8C" />
    <rect x="-45" y="15" width="5" height="10" rx="1" ry="1" fill="#BF616A" />
    <rect x="-45" y="40" width="5" height="10" rx="1" ry="1" fill="#A3BE8C" />
    
    <rect x="-30" y="-60" width="20" height="10" rx="1" ry="1" fill="#3B4252" />
    <rect x="-30" y="-35" width="20" height="10" rx="1" ry="1" fill="#3B4252" />
    <rect x="-30" y="-10" width="20" height="10" rx="1" ry="1" fill="#3B4252" />
    <rect x="-30" y="15" width="20" height="10" rx="1" ry="1" fill="#3B4252" />
    <rect x="-30" y="40" width="20" height="10" rx="1" ry="1" fill="#3B4252" />
    
    <!-- Database cylinder -->
    <g transform="translate(0, 90)">
      <ellipse cx="0" cy="-15" rx="30" ry="15" fill="#4C566A" />
      <rect x="-30" y="-15" width="60" height="30" fill="#4C566A" />
      <ellipse cx="0" cy="15" rx="30" ry="15" fill="#4C566A" />
      
      <!-- Data lines -->
      <ellipse cx="0" cy="-15" rx="20" ry="10" fill="none" stroke="#5E81AC" stroke-width="1" />
      <ellipse cx="0" cy="-5" rx="20" ry="10" fill="none" stroke="#5E81AC" stroke-width="1" />
      <ellipse cx="0" cy="5" rx="20" ry="10" fill="none" stroke="#5E81AC" stroke-width="1" />
    </g>
    
    <!-- Environment Label -->
    <rect x="-75" y="-100" width="150" height="20" rx="5" ry="5" fill="#2E3440" opacity="0.9" />
    <text x="0" y="-85" font-family="Arial, sans-serif" font-weight="bold" font-size="14" fill="#FFFFFF" text-anchor="middle">ON-PREMISES</text>
  </g>
  
  <!-- Cloud infrastructure (right side) -->
  <g transform="translate(600, 225)">
    <!-- Main cloud shape -->
    <path d="M-60,0 Q-70,-40 -40,-50 Q-20,-80 10,-70 Q40,-90 60,-60 Q90,-70 100,-40 Q120,-10 90,10 Q100,50 60,60 Q40,80 10,70 Q-20,90 -40,70 Q-80,80 -90,50 Q-110,20 -80,0 Q-90,-20 -60,0" fill="url(#cloudGradient)" filter="url(#glow)" />
    
    <!-- Cloud servers (simplified) -->
    <g transform="translate(0, 0) scale(0.8)">
      <rect x="-40" y="-30" width="80" height="60" rx="5" ry="5" fill="#5E81AC" />
      <rect x="-35" y="-25" width="70" height="10" rx="2" ry="2" fill="#81A1C1" />
      <rect x="-35" y="-10" width="70" height="10" rx="2" ry="2" fill="#81A1C1" />
      <rect x="-35" y="5" width="70" height="10" rx="2" ry="2" fill="#81A1C1" />
      <rect x="-35" y="20" width="70" height="10" rx="2" ry="2" fill="#81A1C1" />
    </g>
    
    <!-- Environment Label -->
    <rect x="-75" y="-100" width="150" height="20" rx="5" ry="5" fill="#2E3440" opacity="0.9" />
    <text x="0" y="-85" font-family="Arial, sans-serif" font-weight="bold" font-size="14" fill="#FFFFFF" text-anchor="middle">CLOUD</text>
  </g>
  
  <!-- Migration arrow -->
  <g transform="translate(400, 225)">
    <path d="M-120,0 L120,0" stroke="#88C0D0" stroke-width="5" stroke-dasharray="10,5" filter="url(#glow)" />
    <polygon points="120,0 100,-15 100,15" fill="#88C0D0" />
    
    <!-- Animated data flowing across -->
    <g fill="url(#dataGradient)">
      <circle cx="-100" cy="0" r="5">
        <animate attributeName="cx" values="-100;100" dur="3s" repeatCount="indefinite" />
      </circle>
      <circle cx="-40" cy="0" r="5">
        <animate attributeName="cx" values="-40;100" dur="3s" repeatCount="indefinite" />
      </circle>
      <circle cx="20" cy="0" r="5">
        <animate attributeName="cx" values="20;100" dur="3s" repeatCount="indefinite" />
      </circle>
    </g>
    
    <text x="0" y="-20" font-family="Arial, sans-serif" font-weight="bold" font-size="16" fill="#FFFFFF" text-anchor="middle">MIGRATION</text>
  </g>
  
  <!-- Course title -->
  <g filter="url(#glow)">
    <rect x="200" y="380" width="400" height="50" rx="5" ry="5" fill="#2E3440" opacity="0.9" />
    <text x="400" y="415" font-family="Arial, sans-serif" font-weight="bold" font-size="28" fill="#FFFFFF" text-anchor="middle">CLOUD MIGRATION</text>
  </g>
  
  <!-- VerTechie brand -->
  <g transform="translate(60, 40)">
    <rect x="-40" y="-20" width="120" height="40" rx="5" ry="5" fill="#2E3440" opacity="0.8" />
    <text x="20" y="5" font-family="Arial, sans-serif" font-weight="bold" font-size="16" fill="#FFFFFF" text-anchor="middle">VerTechie</text>
  </g>
  
  <!-- Benefits list -->
  <g transform="translate(400, 320)">
    <rect x="-150" y="-20" width="300" height="40" rx="5" ry="5" fill="#2E3440" opacity="0.7" />
    <text x="0" y="5" font-family="Arial, sans-serif" font-weight="bold" font-size="16" fill="#FFFFFF" text-anchor="middle">SCALABILITY • PERFORMANCE • COST SAVINGS</text>
  </g>
  
  <!-- Decorative cloud icons -->
  <g opacity="0.5">
    <path transform="translate(700, 80) scale(0.5)" fill="#FFFFFF" d="M-20,0 Q-30,-20 -10,-25 Q0,-40 15,-35 Q30,-45 40,-30 Q55,-35 60,-20 Q70,-5 55,5 Q60,25 40,30 Q30,40 15,35 Q0,45 -10,35 Q-30,40 -35,25 Q-45,10 -30,0 Q-35,-10 -20,0" />
    <path transform="translate(100, 80) scale(0.5)" fill="#FFFFFF" d="M-20,0 Q-30,-20 -10,-25 Q0,-40 15,-35 Q30,-45 40,-30 Q55,-35 60,-20 Q70,-5 55,5 Q60,25 40,30 Q30,40 15,35 Q0,45 -10,35 Q-30,40 -35,25 Q-45,10 -30,0 Q-35,-10 -20,0" />
    <path transform="translate(750, 350) scale(0.3)" fill="#FFFFFF" d="M-20,0 Q-30,-20 -10,-25 Q0,-40 15,-35 Q30,-45 40,-30 Q55,-35 60,-20 Q70,-5 55,5 Q60,25 40,30 Q30,40 15,35 Q0,45 -10,35 Q-30,40 -35,25 Q-45,10 -30,0 Q-35,-10 -20,0" />
    <path transform="translate(50, 350) scale(0.3)" fill="#FFFFFF" d="M-20,0 Q-30,-20 -10,-25 Q0,-40 15,-35 Q30,-45 40,-30 Q55,-35 60,-20 Q70,-5 55,5 Q60,25 40,30 Q30,40 15,35 Q0,45 -10,35 Q-30,40 -35,25 Q-45,10 -30,0 Q-35,-10 -20,0" />
  </g>
</svg> 